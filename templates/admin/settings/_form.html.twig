<form name="settings" method="post">
    <div id="settings">
        <div><label for="settings_title">Title</label><input type="text" class="form-control" id="settings_title" name="settings[title]"
                maxlength="255"></div>
        <div><label for="settings_keywords">Keywords</label><input type="text" class="form-control" id="settings_keywords"
                name="settings[keywords]" maxlength="255"></div>
        <div><label for="settings_descriptions">Descriptions</label><input type="text" class="form-control" id="settings_descriptions"
                name="settings[descriptions]" maxlength="255"></div>
        <div><label for="settings_company">Company</label><input type="text" class="form-control" id="settings_company"
                name="settings[company]" maxlength="255"></div>
        <div><label for="settings_aboutus">Aboutus</label><input type="text" class="form-control" id="settings_aboutus"
                name="settings[aboutus]" maxlength="255"></div>
        <div><label for="settings_status">Status</label><input type="text" class="form-control" id="settings_status" name="settings[status]"
                maxlength="255"></div>
        <div><label for="settings_created_at">Created at</label><input type="datetime-local" class="form-control" id="settings_created_at"
                name="settings[created_at]"></div>
        <div><label for="settings_updated_at">Updated at</label><input type="datetime-local" class="form-control" id="settings_updated_at"
                name="settings[updated_at]"></div>
    </div>
    <button class="btn">Update</button>
</form>